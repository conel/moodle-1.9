\par /**
\par  * Create groups in a meta course from its child courses, filled with users of passed role id(s)
\par  * Created groups get their name from the child course (fullname)
\par  * This can be run from the cron to daily update members/create groups for all meta courses
\par  *
\par  * @param object    $course    course object
\par  * @param int/array $roles     role id(s) to set which users are added to group - defaults to students
\par  * @return boolean
\par  * @since Moodle 1.9.5 (Build 20090819)
\par  */
\par function groups_create_from_children($course, $roles = 5) \{
\par 
\par     // only run when course is a meta course
\par     if (record_exists("course_meta", "parent_course", $course->id)) \{
\par 
\par         // get all child courses for current meta
\par         $child_courses = get_courses_in_metacourse($course->id);
\par 
\par         // delete all members of current "autocreate" groups, leave manual groups alone
\par         // "autocreate" = if group exists with same name as child course, remove members from these groups only
\par         $del_group_ids = array();
\par         foreach ($child_courses as $child) \{
\par             $mdl_group = get_record_select('groups', "courseid = ".$course->id." AND name = '".$child->fullname."' " );
\par             if (is_object($mdl_group)) \{
\par                 $del_group_ids[] = $mdl_group->id;
\par             \}
\par         \}
\par         // if autocreate groups found
\par         if (count($del_group_ids) > 0) \{
\par             $del_group_ids_csv = implode(',',$del_group_ids);
\par             // Group ids to exclude from member deletion
\par             groups_delete_group_members($course->id, 0, false, $del_group_ids_csv);
\par         \}
\par 
\par         foreach ($child_courses as $child) \{
\par             // Check if groups exist with these names in this course?
\par             $mdl_group = get_record_select('groups', "courseid = ".$course->id." AND name = '".$child->fullname."' " );
\par 
\par             // If course does not exist, create it
\par             if (!is_object($mdl_group)) \{
\par                 $mdl_group = new StdClass;
\par                 $mdl_group->name = $child->fullname;
\par                 $mdl_group->description = '';
\par                 $mdl_group->courseid = $course->id;
\par 
\par                 $group_id = groups_create_group($mdl_group);
\par 
\par                 if (!$mdl_group = get_record('groups', 'id', $group_id))\{
\par                     $error_creating_group = 'Creating group '.$child->fullname.' failed';
\par                     error_log($error_creating_group);
\par                     debugging($error_creating_group);
\par                     continue;
\par                 \}
\par             \}
\par 
\par             $context = get_context_instance(CONTEXT_COURSE, $child->id);
\par             // If user of given role type exist in child course add them to new group - sort by firstname
\par             if ($users_of_role = get_role_users($roles, $context, false, '', 'u.firstname ASC')) \{
\par                 foreach ($users_of_role as $user) \{
\par                     groups_add_member($mdl_group->id, $user->id);
\par                 \}
\par             \}
\par 
\par         \} //foreach child course
\par         return true;
\par 
\par     \} else \{
\par         $not_a_meta_course = 'Course given is not a meta course';
\par         error_log($not_a_meta_course);
\par         debugging($not_a_meta_course);
\par         return false;
\par     \}
\par \}
